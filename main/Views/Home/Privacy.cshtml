@{
    ViewData["Title"] = "Subscribe";
}
<div class="container">
    <div class="card">
        <div class="card-header">
            <h6>Sign In</h6>
        </div>
        <div class="card-body">
            <div class="form-group">
                <label>Email</label>
                <input type="email" class="form-control" placeholder="Email">
            </div>
            <div class="form-group">
                <label>Topic</label>
                <input type="text" class="form-control" placeholder="Topic">
            </div>
        </div>
        <div class="card-footer">
            <a href="http://localhost:8080/proyecto/series/index.html" class="btn btn-primary w-100">Subscribe</a>
        </div>
    </div>
</div>
